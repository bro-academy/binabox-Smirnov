<section class="blog">
    <h2 class="blog__title">{{ blog.title }}</h2>
    <div class="blog__scroller">
        <ul class="blog__list">
            {% for item in blogItems %}
                <li class="blog__list-item">
                    {% include 'partials/blogcard.njk'%}
                </li>

            {% endfor %}
        </ul>
    </div>
    <a href="#" class="blog__link button">{{ blog.link }}</a>
</section>